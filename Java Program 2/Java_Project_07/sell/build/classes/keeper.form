        <AuxValue name="JavaCodeGenerator_InitCodePre" type="java.lang.String" value="ArrayList arr=new ArrayList();&#xa;connector ob1=new connector();&#xa;String query = &quot;Select * FROM product&quot;;&#xa;int i=0;&#xa;try {&#xa;&#xa;        ob1.rs = ob1.stmt.executeQuery(query);&#xa;        String contact = &quot;&quot;;&#xa;        i = 0;&#xa;        while (ob1.rs.next()) {&#xa;                contact = ob1.rs.getString(1);&#xa;                arr.add(contact);&#xa;                contact = ob1.rs.getString(2);&#xa;                arr.add(contact);&#xa;                contact = ob1.rs.getString(4);&#xa;                arr.add(contact);&#xa;                contact = ob1.rs.getString(3);&#xa;                arr.add(contact);&#xa;                i++;&#xa;        }&#xa;        ob1.con.close();&#xa;} catch (SQLException e) {&#xa;        e.printStackTrace();&#xa;}&#xa;String str3[][]=new String[i+5][5],s1,s2,s3,s4,k[]={&quot;No&quot;,&quot;Name&quot;,&quot;Code&quot;,&quot;Price&quot;,&quot;Unit&quot;};&#xa;int ind=0;&#xa;i=0;&#xa;int l=1;&#xa;while(i&lt;arr.size())&#xa;{&#xa;    s1=(String)arr.get(i);&#xa;    s2=(String)arr.get(i+1);&#xa;    s3=(String)arr.get(i+2);&#xa;    s4=(String)arr.get(i+3);&#xa;    str3[ind][0]=&quot;&quot;+l;&#xa;    str3[ind][1]=s1;&#xa;    str3[ind][2]=s2;&#xa;    str3[ind][3]=s3;&#xa;    str3[ind++][4]=s4;&#xa;    i+=4;&#xa;    l++;&#xa;}&#xa;    &#xa;    &#xa;    &#xa;&#xa;&#xa;&#xa;&#xa;javax.swing.JTable tl=new javax.swing.JTable(str3,k);&#xa;tl.setPreferredScrollableViewportSize(new Dimension(450,150));&#xa;tl.setFillsViewportHeight(true);&#xa;scrollPane1.add(tl);"/>
